//- Sidebar
nav#leftSidebar

    .leftSidebar-header
        h3(id="titleSidebar") Informatics Social Network

    ul.list-unstyled.components
        li.active
            //-i.fas.fa-newspaper
            a(href="#") Feed de not√≠cias
        li
            //-i.fas.fa-users
            a.dropdown-toggle(href="#groupSubmenu" data-toggle="collapse" aria-expanded="false") Grupos
            ul#groupSubmenu.collapse.list-unstyled
                each group in user.groups
                    li
                        a(href="#")= group.name
        li
            //-i.fas.fa-calendar
            a.dropdown-toggle(href="#eventSubmenu" data-toggle="collapse" aria-expanded="false") Eventos
            ul#eventSubmenu.collapse.list-unstyled
                each event in user.events
                    li
                        a(href="#")= event.name
